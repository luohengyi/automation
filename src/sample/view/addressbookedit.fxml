<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Addressbookedit">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <TabPane tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="编辑地址薄">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <GridPane layoutX="-1.0" prefHeight="369.0" prefWidth="600.0">
                          <columnConstraints>
                            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="129.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="477.0" minWidth="10.0" prefWidth="243.0" />
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="477.0" minWidth="10.0" prefWidth="229.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints maxHeight="119.0" minHeight="10.0" prefHeight="74.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="233.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="235.0" minHeight="10.0" prefHeight="235.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label text="地址薄名称" />
                              <TextField fx:id="addrName" GridPane.columnIndex="1" />
                              <Button fx:id="determine" mnemonicParsing="false" prefHeight="28.0" prefWidth="85.0" text="确定" GridPane.rowIndex="1" />
                              <Button fx:id="back" mnemonicParsing="false" prefHeight="28.0" prefWidth="85.0" text="返回" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                           </children>
                        </GridPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</GridPane>
